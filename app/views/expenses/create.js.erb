

function refresh2() {
  // console.log('Index JS');
  modalClose();

  $('tbody#expenses-table-body tr').remove();
  $('tbody#expenses-table-body').append('<%= j render "expenses" %>');

  $('#index-bar').remove();
  $('div#index-filters').prepend('<%= j render "indexbar" %>');
  $('body').prepend('<%= j render "ok-create" %>');

  $('#dateFilter').remove();
  $('h4.dropdown').prepend('<%= j render "dateFilter" %>');

}


$(document).on('page:load', refresh2());